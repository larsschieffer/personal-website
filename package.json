{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "prepare": "husky install",
    "build": "remix build",
    "dev": "remix dev",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:write": "eslint --fix .",
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "seed": "ts-node prisma/seed.ts",
    "seed:dev": "dotenv -e config/dev.env pnpm dlx prisma db seed",
    "db-migrate:dev": "dotenv -e config/dev.env pnpm dlx prisma migrate dev",
    "db-management:dev": "dotenv -e config/dev.env pnpm prisma studio",
    "db-management:prod": "dotenv -e config/prod.env pnpm prisma studio"
  },
  "dependencies": {
    "@fontsource/inter": "5.0.3",
    "@fontsource/merriweather": "5.0.3",
    "@octokit/rest": "19.0.13",
    "@prisma/client": "4.15.0",
    "@react-icons/all-files": "4.1.0",
    "@remix-run/css-bundle": "1.17.1",
    "@remix-run/node": "1.17.1",
    "@remix-run/react": "1.17.1",
    "@remix-run/serve": "1.17.1",
    "@remix-run/vercel": "1.17.1",
    "@vercel/node": "2.15.1",
    "@vercel/remix": "1.17.0",
    "esbuild": "0.18.4",
    "feed": "4.2.2",
    "flat": "5.0.2",
    "highlight.js": "11.8.0",
    "http-status-codes": "2.2.0",
    "isbot": "3.6.10",
    "mdx-bundler": "9.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4",
    "react-toastify": "9.1.3",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.17.1",
    "@remix-run/eslint-config": "1.17.1",
    "@remix-run/serve": "1.17.1",
    "@testing-library/react": "14.0.0",
    "@types/flat": "5.0.2",
    "@types/node": "20.2.5",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vitest/coverage-v8": "0.32.2",
    "autoprefixer": "10.4.14",
    "concurrently": "8.2.0",
    "dotenv": "16.2.0",
    "dotenv-cli": "7.2.1",
    "esbuild-register": "3.4.2",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-formatjs": "4.10.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-vitest": "0.2.6",
    "happy-dom": "9.20.3",
    "husky": "8.0.3",
    "msw": "1.2.2",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prisma": "4.15.0",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  },
  "engines": {
    "node": "18.16.0"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
