{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "prepare": "husky install",
    "build": "remix build",
    "dev": "dotenv -e config/dev.env remix dev",
    "typecheck": "tsc",
    "lint": "eslint --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:write": "eslint --fix .",
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "seed": "ts-node prisma/seed.ts",
    "seed:dev": "dotenv -e config/dev.env pnpm dlx prisma db seed",
    "db-migrate:dev": "dotenv -e config/dev.env pnpm dlx prisma migrate dev",
    "db-management:dev": "dotenv -e config/dev.env pnpm prisma studio",
    "db-management:prod": "dotenv -e config/prod.env pnpm prisma studio"
  },
  "dependencies": {
    "@fontsource/inter": "5.0.17",
    "@fontsource/merriweather": "5.0.12",
    "@octokit/rest": "20.0.2",
    "@prisma/client": "5.11.0",
    "@react-icons/all-files": "4.1.0",
    "@remix-run/css-bundle": "2.8.1",
    "@remix-run/node": "2.8.1",
    "@remix-run/react": "2.8.1",
    "@remix-run/serve": "2.8.1",
    "@remix-run/vercel": "1.19.3",
    "@vercel/node": "3.0.24",
    "@vercel/remix": "2.8.1",
    "esbuild": "0.20.2",
    "feed": "4.2.2",
    "flat": "6.0.1",
    "highlight.js": "11.9.0",
    "http-status-codes": "2.3.0",
    "isbot": "5.1.2",
    "mdx-bundler": "10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.6.3",
    "react-toastify": "10.0.5",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "2.8.1",
    "@remix-run/serve": "2.8.1",
    "@testing-library/react": "14.2.2",
    "@types/flat": "5.0.5",
    "@types/node": "20.11.30",
    "@types/react": "18.2.71",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@vitest/coverage-v8": "0.34.6",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.1",
    "esbuild-register": "3.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-formatjs": "4.12.2",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-vitest": "0.3.10",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "msw": "2.2.11",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.12",
    "prisma": "5.11.0",
    "tailwindcss": "3.4.1",
    "ts-node": "10.9.2",
